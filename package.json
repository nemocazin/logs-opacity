{
    "name": "logs-opacity",
    "displayName": "logs-opacity",
    "description": "VScode extension for logs opacity.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.108.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "logs-opacity.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "vscode:prepublish": "npm run compile",
        "vscode:package": "vsce package"
    },
    "devDependencies": {
        "@types/vscode": "^1.108.1",
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "typescript-eslint": "^8.52.0",
        "eslint": "^9.39.2",
        "esbuild": "^0.27.2",
        "npm-run-all": "^4.1.5",
        "typescript": "^5.9.3",
        "@vscode/test-cli": "^0.0.12",
        "@vscode/test-electron": "^2.5.2"
    }
}
